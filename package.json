{
  "name": "web3-webpacked",
  "version": "1.0.11",
  "description": "Drop-in web3 solution for single-page Ethereum dApps",
  "main": "dist/node/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-web": "npx webpack --watch",
